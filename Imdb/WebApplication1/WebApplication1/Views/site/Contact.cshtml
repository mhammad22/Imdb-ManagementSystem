@{
    Layout = null;
}
@model string
<!DOCTYPE html>
<html>
<head>
    <script src="~/Scripts/signup.js"></script>


    <link rel="stylesheet" type="text/css" href="~/Content/Site.css" />
    <title>.:ContactUs:.</title>
</head>
<body>
    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a href="../../site"><img onclick="" src="~/Content/Logo1.png" width="130" height="120"></a>
    <!-- inline CSS on div tag;  external CSS on text boxes;-->
    <div class="form" style="padding-left:125px;padding-right:125px;padding-top:60px;margin-left:375px;margin-right:375px;">

        <!--<h1 style="font-size:3vw">Demo Lab 2</h1>
        <br />

        <a href="~/Views/Home/allStudents.cshtml">Click here to see all students</a>
            -->
        <h1 class="monospace">Contact Us!</h1>
        <br />
        <br />


        <form onsubmit="return !!(ValidateEmail());" method="post" action="../site/Feedback">


            <p class="monospace" for="lname">Message*</p>
            <textarea id="subject" name="message" required placeholder="Write something.." style="height:200px"></textarea>
            <span id="l"></span>
            <p class="monospace" for="fname">Full Name*</p>
            <input type="text" id="fname" required name="fname" placeholder="Mehboob">
            <span id="fn"></span>
            <p class="monospace" for="email">Email*</p>
            <input type="text" id="email" required name="emailname" placeholder="userName@imdb.com">
            <span id="e"></span>
            <br />
            <p class="monospace">
                <label>@Model</label>
            </p>
            <br />
            <br />

            <button type="submit" class="button button-block">REGISTER</button>

        </form>


    </div>
</body>
</html>